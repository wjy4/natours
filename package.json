{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Express, node and mongodb",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "nodemon --inspect server.js & sleep 2 && open -a /Applications/Chromium.app http://localhost:9229",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/build --no-content-hash",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/build --no-content-hash"
  },
  "author": "Scott",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.23",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "regenerator-runtime": "^0.14.1",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6",
    "stripe": "^18.0.0",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "parcel": "^2.14.4",
    "prettier": "^3.5.2"
  },
  "engines": {
    "node": "18.x"
  }
}
